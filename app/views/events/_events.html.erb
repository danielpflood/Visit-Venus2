<table id="events">
<thead>
  <tr>
    <th><%= sortable "name", "Name" %></th>
    <th><%= sortable "eventtype", "Type" %></th>
    <th><%= sortable "location", "Location" %></th>
    <th><%= sortable "startdate", "Start" %></th>
    <th><%= sortable "enddate", "End" %> </th>
  </tr></thead>
<tbody>
<% @events.each do |event| %>
  <tr>
    <td><%= event.name %></td>	
    <td><%= event.eventtype %></td>
    <td><%= event.location %></td>
    <td><%= event.startdate %></td>
    <td><%= event.enddate %></td>
  </tr>
<% end %>
</tbody>
</table>
<%= will_paginate @events %>